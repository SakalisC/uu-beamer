\ProvidesPackage{beamercolorthemeUU.SE}
	[2015/11/09 v2.0 beamer 'UU.SE' colour theme]

\mode<presentation>

\definecolor{UU.SEblack}{RGB}{0,0,0}
\definecolor{UU.SEred}{RGB}{150,0,0}
\definecolor{UU.SEblue}{RGB}{0,76,103}
\definecolor{UU.SEgrey}{RGB}{217,217,217}
\definecolor{UU.SEdarkgrey}{RGB}{128,128,128}
\definecolor{UU.SEorange}{RGB}{255,160,35}

\setbeamercolor{normal text}{fg=UU.SEblack}
\setbeamercolor{alerted text}{fg=UU.SEorange}
\setbeamercolor{example text}{fg=UU.SEblue}
\setbeamercolor{structure}{fg=UU.SEred}
\setbeamercolor{footer bar}{bg=UU.SEgrey}
\setbeamercolor{footer text}{fg=UU.SEdarkgrey}

\setbeamercolor{author}{fg=UU.SEblack}
\setbeamercolor{date}{fg=UU.SEblack}
\setbeamercolor{framesubtitle}{fg=UU.SEred}

\mode<all>
